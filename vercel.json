{
    "version": 2,
    "routes": [
      {
        "src": "/portfolio/assets/(.*)",
        "dest": "/portfolio/assets/$1"
      },
      {
        "src": "/portfolio/(.*)",
        "dest": "/portfolio/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  