{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "docs"
        }
      }
    ],
    "routes": [
      {
        "src": "/portfolio/(.*)",
        "dest": "/portfolio/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  